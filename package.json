{
  "name": "processwire-dashboard",
  "version": "1.5.2",
  "description": "Configurable dashboard for ProcessWire's admin interface",
  "scripts": {
    "dev": "parcel src/*.js src/*.css --dist-dir . --no-source-maps --no-hmr",
    "build": "parcel build src/*.js src/*.css --dist-dir . --no-source-maps",
    "bump": "bump package.json package-lock.json VERSION *.module --commit \"Release v%s\" --tag \"v\"",
    "docs": "docsify serve docs"
  },
  "author": "Philipp Daun",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.7",
    "parcel": "^2.5.0",
    "postcss": "^8.4.13",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.5.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "version-bump-prompt": "^6.1.0"
  }
}
