{
  "name": "processwire-dashboard",
  "version": "1.0.3",
  "description": "Configurable dashboard for ProcessWire's admin interface",
  "main": "\"\"",
  "scripts": {
    "dev": "parcel src/*.js src/*.css --out-dir . --no-source-maps --no-hmr",
    "build": "parcel build src/*.js src/*.css --out-dir . --no-source-maps",
    "bump": "bump package.json package-lock.json VERSION *.module --commit \"Release v%s\" --tag \"v\"",
    "docs": "docsify serve docs"
  },
  "author": "Philipp Daun",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "cssnano": "^4.1.10"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "lodash": "^4.17.21",
    "postcss-custom-properties": "^9.1.1",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0"
  }
}
